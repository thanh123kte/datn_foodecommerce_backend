# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=qtifood_db
DB_USERNAME=postgres
DB_PASSWORD=your_password_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_minimum_256_bits
